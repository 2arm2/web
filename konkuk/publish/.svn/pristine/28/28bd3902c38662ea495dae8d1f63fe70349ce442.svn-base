#contents .employment_statistics_modify .search_result_wrap{width:918px;}
#contents .employment_statistics_modify .search_result{min-height:auto;}
#contents .employment_statistics_modify .search_result .graduate_wrap{height:200px;}
#contents .employment_statistics_modify .data_detail_wrap{width:918px;}
#contents .code_management input{width:238px;}
#contents .code_management .btn_wrap a{display:inline-block;width:100px;height:36px;line-height:36px;border-radius:5px;background-color:#6a6c6f;color:#FFF;font-size:13px;}
#contents .code_management_view .btn_wrap a{display:inline-block;width:100px;height:36px;line-height:36px;border-radius:5px;border:1px solid #6a6c6f;font-size:13px;margin:0 3px;}
#contents .code_management_view .btn_wrap a:first-child{background-color:#ea6557;color:#FFF;border:1px solid #ea6557;}
#contents .code_detail_wrap{margin-top:100px;}
#contents .code_detail_wrap .search_wrap{text-align:right;margin-bottom:20px;}
#contents .code_detail_wrap .search_wrap input{border:1px solid #eaeaea;width:228px;height:38px;padding:0 5px;}
#contents .code_detail_wrap .search_wrap input:focus{border:1px solid #6a6c6f;}
#contents .code_detail_wrap .search_wrap button{background-color:#6a6c6f;color:#FFF;width:60px;height:38px;border-radius:5px;margin:0 3px;}
#contents .code_detail_wrap .code_detail{height:300px;overflow-y:scroll;}
#contents .code_management_modify .file_add label{position:relative;cursor:pointer;display:inline-block;vertical-align:middle;overflow:hidden;width:60px;height:38px;line-height:38px;border-radius:5px;background:#34495e;color:#fff;text-align:center;}
#contents .code_management_modify .file_add label input{position:absolute;width:1px;height:1px;margin:-1px;padding:0;border:0;clip:rect(0 0 0 0);overflow:hidden;}
#contents .code_management_modify .file_add input[type=text]{border:1px solid #eaeaea;width:228px;height:38px;padding:0 5px;}
#contents .code_management_modify .btn_wrap a{display:inline-block;width:100px;height:36px;line-height:36px;border-radius:5px;border:1px solid #6a6c6f;font-size:13px;margin:0 3px;}
#contents .code_management_modify .btn_wrap a:first-child{background-color:#ea6557;color:#FFF;border:1px solid #ea6557;}
#contents .code_management_modify td a{text-decoration:underline;color:#3498db;}
#contents .file_download{min-height:367px;}
#contents .file_download .select_box.input_status{width:80px;}


/* Error */
.error_page_wrap{position:absolute;top:50%;left:50%;margin:-200px 0 0 -300px;width:600px;height:400px;}
.error_page{background-color:#FFF;padding:50px;box-shadow:0px 6px 12px rgba(0,0,0,0.175);text-align:center;}
.error_page h1{color:#1e863e;font-size:25px;margin-bottom:30px;}
.error_page p{line-height:20px;}
.error_page .link_wrap{margin-top:50px;}
.error_page .link_wrap a{display:inline-block;width:120px;height:36px;line-height:36px;border-radius:5px;margin:0 3px;border:1px solid #6a6c6f;}
.error_page .link_wrap a.go_home{background-color:#6a6c6f;color:#FFF;}
.error_page_wrap .footer{margin-top:20px;text-align:center;font-size:11px;letter-spacing:0;}
.error_page_wrap .footer dt{display:inline-block;}
.error_page_wrap .footer dd{display:inline-block;}

/* Button */
.btn_wrap{text-align:center;margin-top:25px;}
.btn_wrap button{width:100px;height:36px;border-radius:5px;margin:0 3px;}
.btn_wrap .btn_reset{border:1px solid #6a6c6f;}
.btn_wrap .btn_search{background-color:#6a6c6f;color:#FFF;}
.btn_wrap .save_excel{background-color:#74d248;color:#FFF;}
.btn_wrap .btn_print, .btn_wrap .save_pdf{background-color:#3498db;color:#FFF;}
.btn_wrap .btn_submit{background-color:#ea6557;color:#FFF;}
.btn_wrap .btn_closed{background-color:#6a6c6f;color:#FFF;}
.btn_application{background-color:#34495e;color:#FFF;width:60px;height:39px;border-radius:5px;margin:0 3px;}

/* Select-Box */
.select_box{position:relative;display:inline-block;vertical-align:middle;width:238px;height:38px;border:1px solid #eaeaea;z-index:1;}
.select_box.focus{border-color:#6a6c6f;}
.select_box::before{content:'';position:absolute;top:50%;right:15px;width:0;height:0;margin-top:-1px;border-left:5px solid transparent;border-right:5px solid transparent;border-top:5px solid #6a6c6f;}
.select_box p{position:absolute;top:1px;left:5px;padding:.9em;color:#6a6c6f;z-index:-1;font-size:12px;}
.select_box select{width:100%;height:auto;line-height:normal;padding:.9em;border:0;opacity:0;filter:alpha(opacity=0);-webkit-appearance:none;-moz-appearance:none;appearance:none;}

/* Layer-Popup */
.layer_popup_wrap{display:none;}
.layer_mask{position:absolute;left:0;top:0;background-color:#000;z-index:9000;}
.layer_popup{position:fixed;left:50%;top:50%;width:400px;height:560px;margin:-280px 0 0 -200px;background-color:#f1f3f6;z-index:10000;text-align:center;}
.layer_popup h4{font-size:20px;line-height:50px;padding:0 20px;text-align:left;background-color:#74d248;color:#FFF;margin-bottom:40px;}
.layer_popup > ul{margin-top:40px;padding:0 30px;height:360px;overflow-y:scroll;}
.layer_popup > ul > li{background-color:#FFF;margin-top:10px;}
.layer_popup > ul > li:first-child{margin-top:0;}
.layer_popup > ul > li p{padding:20px 0;}
.layer_popup > ul > li a{display:block;width:90%;height:100%;padding:5%;border:1px solid #eaeaea;}
.layer_popup > ul > li a:hover, .layer_popup > ul > li a:focus, .layer_popup > ul > li a:active{border:1px solid #000;}
.layer_popup dl{text-align:left;margin-top:5px;}
.layer_popup dl:first-child{margin-top:0;}
.layer_popup dt{float:left;width:60px;font-size:11px;line-height:18px;color:#000;}
.layer_popup dd{float:left;width:221px;}
.layer_popup > ol{margin-top:40px;padding:0 30px;height:300px;overflow-y:scroll;}
.layer_popup > ol > li{background-color:#FFF;margin-top:10px;border:1px solid #eaeaea;text-align:left;padding:10px;}
.layer_popup > ol > li:first-child{margin-top:0;}
.layer_popup .certificate_list li{float:left;width:50%;font-size:12px;margin:3px 0;}
#contents .employment_statistics .data_detail_wrap .layer_popup h5{border-bottom:1px solid #eaeaea;margin-bottom:10px;padding-bottom:10px;font-weight:bold;}
.layer_popup .layer_popup_closed{position:absolute;top:0;right:0;width:50px;height:50px;background-color:#FFF;color:#74d248;font-size:30px;}
.view_rights_popup ul{height:430px;}
.view_rights_popup li{padding:20px;}
.view_rights_popup dt{width:100px;}
.view_rights_popup dd{width:181px;}

/* Popup-Graph */
.bg_popup{background-color:#FFF;}
.popup_wrap{width:1200px;margin:50px auto;}
.popup_wrap h1{text-align:center;font-size:20px;color:#000;margin-bottom:20px;}
.popup_wrap h2{font-size:15px;}
.popup_wrap h3{font-size:15px;line-height:15px;margin:10px 0 50px 0;border-left:3px solid #6a6c6f;padding-left:7px;}
.popup_wrap .chart_wrap > div{width:560px;float:left;margin:0 20px;}
.popup_wrap .chart_wrap h4, .popup_wrap .one_chart h4{text-align:center;margin-bottom:20px;text-decoration:underline;}
.popup_wrap .one_chart{width:900px;margin:0 auto;}
.popup_wrap canvas{margin-bottom:100px;}
.popup_wrap .three_chart > div{width:30%;}

/* Footer */
#footer{height:50px;line-height:50px;border-top:1px solid #eaeaea;text-align:center;letter-spacing:0;background-color:#FFF;}
#footer dl{width:100%;margin:0 auto;font-size:0;}
#footer dt{display:inline-block;font-size:11px;}
#footer dd{display:inline-block;font-size:11px;}

/* Media Query */
@page{
	size:auto;margin:0;
}

@media print{
	.btn_wrap{display:none;}
	.popup_wrap h2, .popup_wrap h3{margin-left:30px;}
	#header{display:none;}
	#contents{padding-bottom:50px;}
	#contents .contents{float:none;width:90%;margin:0 auto;}
	#contents .title{display:none;}
	#contents .contents_title{border:none;margin-top:50px;}
	#contents .search_result{border:none;}
	#contents .navi{display:none;}
	#contents .search_condition_wrap{display:none;}
	#contents .search_result_wrap h4, #contents .search_result_wrap h5{display:none;}
	#contents .vouchers_check tr.off{display:table-row;}
	#footer{display:none;}
}

@media all and (max-width:1600px){
	#header .header{width:100%;}
	#header .header h1{width:95%;padding-left:5%;}
	#header .header ul{width:95%;padding-left:5%;margin-top:30px;}
	#header .admin_data_input a{height:86px;line-height:86px;}
}
@charset "utf-8";

@font-face {
  font-family: 'notokr-medium';
  font-style: normal;
  font-weight: normal;
  src: url(../fonts/notokr-medium.eot);
  src: url(../fonts/notokr-medium.eot?#iefix) format('embedded-opentype'),
       url(../fonts/notokr-medium.woff2) format('woff2'),
       url(../fonts/notokr-medium.woff) format('woff'),
       url(../fonts/notokr-medium.ttf) format('truetype');
	   url(../fonts/notokr-medium.svg#notokr-medium) format('svg');
}

/* Base */
html,body,div,span,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,blockquote,p,address,pre,cite,form,fieldset,legend,input,label,button,textarea,select,table,caption,thead,tfoot,tbody,tr,th,td,img{margin:0;padding:0;}
body,table,th,tr,td,select,input,div,form,button,textarea,h1,h2,h3,h4,h5,h6{font-family:'notokr-medium','Malgun Gothic','나눔고딕','Nanum Gothic','돋움',dotum,sans-serif;color:#6a6c6f;font-size:13px;letter-spacing:-1px;}
h1,h2,h3,h4,h5,h6{font-weight:normal;}
img{vertical-align:top;}
img,fieldset,input{border:0;}
input,select,button{vertical-align:middle;}
input,textarea{outline-style:none;}
button{cursor:pointer;background:none;border:0;}
li{list-style:none;}
address{font-style:normal;}
table{border-collapse:collapse;}
a{text-decoration:none;}
a:link{color:#6a6c6f;}
a:visited{color:#6a6c6f;}
a:hover{color:#6a6c6f;}
a:active{color:#6a6c6f;}
::selection{background:#6a6c6f;color:#fff;}
::-moz-selection{background:#6a6c6f;color:#fff;}
::-webkit-selection{background:#6a6c6f;color:#fff;}
input[type=checkbox], input[type=radio]{cursor:pointer;}
input, select{letter-spacing:0;}

/* Clearfix */
.clearfix{*zoom:1;}
.clearfix:after{display:block;clear:both;content:'';}

/* Layout */
body{background-color:#f1f3f6;}
.ellipsis{overflow:hidden;white-space:nowrap;text-overflow:ellipsis;}
.line_clamp{overflow:hidden;text-overflow:ellipsis;white-space:normal;line-height:20px;height:40px;word-wrap:break-word;display:-webkit-box;-webkit-line-clamp:2;-webkit-box-orient:vertical;}
.hidden{position:absolute;width:1px;height:1px;margin:-1px;padding:0;border:0;clip:rect(0 0 0 0);overflow:hidden;}
a:hover, a:active, a:focus{-webkit-transition:All 0.5s ease;-moz-transition:All 0.5s ease;-o-transition:All 0.5s ease;transition:All 0.5s ease;}

/* Login */
.bg_login{background-color:#e8e8e8;}
.bg_login .login_wrap{background:#1e863e url('../../images/login/bg_login.png') center center no-repeat;height:400px;}
.bg_login .login_wrap .login{position:absolute;top:50%;left:50%;margin:-280px 0 0 -230px;background-color:#FFF;width:360px;height:460px;padding:50px;text-align:center;box-shadow:0px 6px 12px rgba(0,0,0,0.175);}
.bg_login .login_wrap h2{margin-top:5px;color:#000;font-size:17px;}
.bg_login .login_wrap h3{color:#1e863e;text-align:left;margin:40px 0;}
.bg_login .login_wrap dt{text-align:left;color:#888;}
.bg_login .login_wrap input{width:360px;border-bottom:1px solid #888;margin:10px 0 35px;padding-bottom:5px;}
.bg_login .login_wrap input:focus, .login_wrap input:active{border-bottom:1px solid #000;}
.bg_login .login_wrap button{width:360px;height:50px;background-color:#1e863e;color:#FFF;font-size:18px;}
.bg_login .footer{text-align:center;font-size:11px;margin-top:440px;letter-spacing:0;}
.bg_login .footer dt{display:inline-block;}
.bg_login .footer dd{display:inline-block;}

/* Header */
#header{position:relative;background-color:#FFF;}
#header .header{width:1200px;margin:0 auto;padding:50px 0 20px;}
#header .header h1{float:left;font-size:20px;line-height:15px;margin-right:50px;}
#header .header h1 a{color:#000;font-weight:bold;}
#header .header li{float:left;margin-left:20px;padding-left:20px;border-left:1px solid #d6d7da;line-height:15px;}
#header .header li:first-child{margin-left:0;padding-left:0;border-left:0;}
#header .header li a{font-size:15px;}
#header .header li.on a{color:#21ba31;}
#header .admin_data_input{position:absolute;top:30px;right:0;}
#header .admin_data_input a{display:block;width:300px;height:56px;line-height:56px;font-size:15px;background-color:#21ba31;color:#FFF;text-align:center;}
#header .top{position:absolute;left:0;top:0;width:100%;height:30px;line-height:30px;background-color:#000;text-align:right;}
#header .top a{padding-right:20px;border-right:1px solid #2a2a2a;font-size:12px;color:#FFF;}
#header .top button{margin:0 50px 0 20px;font-size:12px;color:#FFF;}

/* Contents */
#contents .title{background:url('../../images/common/bg_title.jpg') no-repeat center center;background-size:cover;width:100%;height:180px;line-height:180px;text-align:center;}
#contents .title h2{color:#FFF;font-size:30px;}
#contents .contents_wrap{width:1200px;margin:0 auto;}
#contents .navi{float:left;width:234px;margin-top:50px;}
#contents .navi h2{background-color:#21ba31;color:#FFF;padding:5px 20px;}
#contents .navi > ul > li{background-color:#FFF;border:1px solid #eaeaea;margin-top:-1px;}
#contents .navi li h3{padding:0 20px;border-left:3px solid #FFF;border-right:3px solid #FFF;}
#contents .navi li.on h3{border-left:3px solid #21ba31;}
#contents .navi h3 a{display:block;width:100%;padding:10px 0;}
#contents .navi li.on h3 a{color:#21ba31;}
#contents .navi h4{padding-left:30px;}
#contents .navi h4 a{display:block;width:100%;padding:5px 0;}
#contents .navi .sub{display:none;padding-bottom:10px;}
#contents .navi .on .sub{display:block;border-left:3px solid #21ba31;}
#contents .navi .sub .on a{color:#21ba31;}
#contents .contents{float:right;width:916px;margin:50px 0 0 50px;}
#contents .contents_title{background-color:#FFF;border:1px solid #eaeaea;padding:25px;margin-bottom:50px;}
#contents .contents_title h3{font-size:20px;line-height:20px;padding-left:15px;border-left:3px solid #21ba31;}
#contents .contents_title p{padding-left:20px;margin-top:10px;}
#contents .search_condition_wrap h5{margin:5px;}
#contents .search_condition{background-color:#FFF;border:1px solid #eaeaea;padding:25px;margin-bottom:50px;}
#contents .search_condition label{color:#000;}
#contents .search_condition dl{margin:10px 0 0 30px;}
#contents .search_condition dl:first-child{margin-top:0;}
#contents .search_condition dt{float:left;width:100px;padding-right:10px;text-align:right;line-height:38px;}
#contents .search_condition dd{float:left;}
#contents .search_condition .select_date .select_box{width:89px;}
#contents .search_condition .select_date span{margin-left:5px;}
#contents .search_condition input{border:1px solid #eaeaea;height:38px;padding:0 5px;}
#contents .search_condition input:focus{border:1px solid #6a6c6f;}
#contents .search_result_wrap h5{margin:5px;}
#contents .search_result{background-color:#FFF;border:1px solid #eaeaea;padding:25px;margin-bottom:50px;}
#contents .search_result table{width:100%;}
#contents .search_result th{color:#000;font-weight:normal;height:50px;line-height:50px;text-align:right;padding:0 20px;}
#contents .search_result th:first-child, #contents .search_result .major{text-align:center;}
#contents .search_result td{font-size:15px;border-bottom:1px solid #eaeaea;height:50px;line-height:50px;text-align:right;letter-spacing:0;padding:0 20px;}
#contents .search_result tfoot td{border-bottom:0;color:#000;}
#contents .search_result th.college{background-color:#FFF;border-right:1px solid #eaeaea;color:#000;line-height:20px;font-size:13px;}
#contents .search_result tbody{border-bottom:1px solid #eaeaea;}
#contents .search_result tbody tr:nth-child(odd){background-color:#f9f9f9;}
#contents .search_result tbody tr{cursor:pointer;}
#contents .search_result tbody tr:hover, #contents .search_result tbody tr:active, #contents .search_result tbody tr:focus{background-color:#eeffe7;}
#contents .search_result tbody th{color:#6a6c6f;font-size:15px;border-bottom:1px solid #eaeaea;}
#contents .search_result .scope_colgroup thead th{height:30px;line-height:30px;text-align:center;border-top:1px solid #eaeaea;}
#contents .search_result .scope_colgroup thead tr:first-child th{border-top:0;}
#contents .search_result .scope_colgroup tbody th[scope=rowgroup]{background-color:#FFF;border-right:1px solid #eaeaea;color:#000;font-size:13px;}
#contents .search_result .scope_colgroup th, #contents .search_result .scope_colgroup td{text-align:center;}
#contents .search_result p{text-align:center;}

/* Data-Input */
#contents .data_input_wrap{width:1200px;margin:50px auto;}
#contents .data_input_wrap .search_condition dt{width:auto;text-align:left;margin-left:20px;}
#contents .data_input_wrap .search_condition dl + dt{margin-left:0;}
#contents .data_input_wrap .search_condition .select_box{width:188px;}
#contents .data_input_wrap .search_condition .select_student{width:78px;}
#contents .data_input_wrap .search_result .table_wrap{height:300px;overflow-y:scroll;}
#contents .data_input_wrap .search_result th{text-align:center;}
#contents .data_input_wrap .search_result td{text-align:center;}

/* Error */
.error_page_wrap{position:absolute;top:50%;left:50%;margin:-200px 0 0 -300px;width:600px;height:400px;}
.error_page{background-color:#FFF;padding:50px;box-shadow:0px 6px 12px rgba(0,0,0,0.175);text-align:center;}
.error_page h1{color:#1e863e;font-size:25px;margin-bottom:30px;}
.error_page p{line-height:20px;}
.error_page .link_wrap{margin-top:50px;}
.error_page .link_wrap a{display:inline-block;width:120px;height:36px;line-height:36px;border-radius:5px;margin:0 3px;border:1px solid #6a6c6f;}
.error_page .link_wrap a.go_home{background-color:#6a6c6f;color:#FFF;}
.error_page_wrap .footer{margin-top:20px;text-align:center;font-size:11px;letter-spacing:0;}
.error_page_wrap .footer dt{display:inline-block;}
.error_page_wrap .footer dd{display:inline-block;}

/* Button */
.btn_wrap{text-align:center;margin-top:25px;}
.btn_wrap button{width:100px;height:36px;border-radius:5px;margin:0 3px;}
.btn_wrap .btn_reset{border:1px solid #6a6c6f;}
.btn_wrap .btn_search{background-color:#6a6c6f;color:#FFF;}
.btn_wrap .save_excel{background-color:#74d248;color:#FFF;}
.btn_wrap .save_pdf{background-color:#3498db;color:#FFF;}
.btn_wrap .btn_submit{background-color:#34495e;color:#FFF;}
.btn_wrap .btn_closed{background-color:#6a6c6f;color:#FFF;}

/* Select-Box */
.select_box{position:relative;display:inline-block;vertical-align:middle;width:238px;height:38px;border:1px solid #eaeaea;z-index:1;}
.select_box.focus{border-color:#6a6c6f;}
.select_box::before{content:'';position:absolute;top:50%;right:15px;width:0;height:0;margin-top:-1px;border-left:5px solid transparent;border-right:5px solid transparent;border-top:5px solid #6a6c6f;}
.select_box p{position:absolute;top:1px;left:5px;padding:.9em;color:#6a6c6f;z-index:-1;font-size:12px;}
.select_box select{width:100%;height:auto;line-height:normal;padding:.9em;border:0;opacity:0;filter:alpha(opacity=0);-webkit-appearance:none;-moz-appearance:none;appearance:none;}

/* Popup */
.bg_popup{background-color:#FFF;}
.popup_wrap{width:1200px;margin:50px auto;}
.popup_wrap h1{text-align:center;font-size:20px;color:#000;margin-bottom:20px;}
.popup_wrap h2{font-size:15px;}
.popup_wrap h3{font-size:15px;line-height:15px;margin:10px 0 50px 0;border-left:3px solid #6a6c6f;padding-left:7px;}
.popup_wrap canvas{margin-bottom:100px;}
.popup_wrap .table_wrap{margin:30px 0 100px;}
.popup_wrap table{width:100%;}
.popup_wrap .table_wrap table{margin-top:20px;}
.popup_wrap caption{text-align:left;margin:10px 5px;}
.popup_wrap th{background-color:#f9f9f9;border:1px solid #eaeaea;padding:8px 0;border-right:0;font-weight:normal;}
.popup_wrap td{border:1px solid #eaeaea;padding-left:10px;letter-spacing:0;border-left:0;color:#000;}
.popup_wrap .kustat caption{color:#21ba31;}
.popup_wrap .kustat th{background-color:#eeffe7;}
.popup_wrap .kustat_wrap > button{width:200px;height:36px;background-color:#e74c3c;color:#FFF;}
.popup_wrap .kustat_data_input{display:none;border:1px solid #e74c3c;padding:25px;}
.popup_wrap .kustat_data_input th{padding:20px 0;background-color:#fff5f5;}
.popup_wrap .kustat_data_input .select_box p{font-size:13px;top:0;color:#000;}
.popup_wrap .kustat_data_input input[type=text]{width:300px;height:38px;border:1px solid #eaeaea;padding:0 5px;}
.popup_wrap .kustat_data_input input:focus{border:1px solid #6a6c6f;}
.popup_wrap .kustat_data_input td label{cursor:pointer;margin:0 20px 0 5px;}
.popup_wrap .kustat_data_input select, .popup_wrap .kustat_data_input input{color:#000;}
.popup_wrap .chart_wrap > div{width:560px;float:left;margin:0 20px;}
.popup_wrap .chart_wrap h4, .popup_wrap .one_chart h4{text-align:center;margin-bottom:20px;text-decoration:underline;}
.popup_wrap .one_chart{width:900px;margin:0 auto;}

/* Footer */
#footer{height:50px;line-height:50px;border-top:1px solid #eaeaea;text-align:center;letter-spacing:0;background-color:#FFF;}
#footer dl{width:100%;margin:0 auto;font-size:0;}
#footer dt{display:inline-block;font-size:11px;}
#footer dd{display:inline-block;font-size:11px;}

/* Media Query */
@page{
size:auto;margin:0;
}
@media print{
.btn_wrap{display:none;}
.popup_wrap h2, .popup_wrap h3{margin-left:30px;}
}

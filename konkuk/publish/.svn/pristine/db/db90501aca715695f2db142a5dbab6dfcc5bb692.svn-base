									<!-- /제외인정자 -->
									<!-- 공통 -->
									<tr>
										<td colspan="4" style="color:#3498db;font-size:11px;">공통</td>
									</tr>
									<tr>
										<th scope="row"><label for="toeicScore">TOEIC</label></th>
